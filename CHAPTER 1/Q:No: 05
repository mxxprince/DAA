def find_max(nums):
    if not nums:
        return None  
    max_value = nums[0]
    for num in nums:
        if num > max_value:
            max_value = num
    return max_value

# Test Cases
print(find_max([1, 2, 3, 4, 5]))  # Expected Output: 5
print(find_max([7, 7, 7, 7, 7]))  # Expected Output: 7
print(find_max([-10, 2, 3, -4, 5]))  # Expected Output: 5
